
#!/bin/bash -e
set -o xtrace
TESTRUN_OUT=/tmp/testrun.log

ifconfig

# Setup requirements
sudo apt-get update
sudo apt-get install openvswitch-common openvswitch-switch 

sudo /usr/share/openvswitch/scripts/ovs-ctl start
sleep 10 
sudo ovs-vsctl add-br br0
sleep 10
sudo ovs-vsctl add-port br0 eth0
sleep 10
echo done here
